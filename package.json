{
  "name": "athammer",
  "version": "0.0.0",
  "author": "Aaron T Hammer athammer@buffalo.edu",
  "description": "Buying pixels for cash and stuff",
  "main": "server.js",
  "scripts": {
    "start": "set NODE_ENV=dev & npm run sass & npm run watch-sass & webpack -d --watch --output-pathinfo & pm2 start ecosystem.config.js",
    "kill": "pm2 kill && pkill node & pkill node-sass & pkill npm",
    "globals": "npm install pm2 -g",
    "logs": "pm2 logs",
    "build": "webpack -p",
    "webpack": "webpack",
    "webpack:watch": "webpack -d --watch",
    "lint": "npm run lint-react && npm run lint-node",
    "lint-react": "./node_modules/.bin/eslint ./src/react/**/*.jsx",
    "lint-node": "./node_modules/.bin/eslint ./server.js ./lib/**/*.js ./db/**/*.js ./routes/**/*.js",
    "watch-sass": "node-sass -w src/sass/main.scss -o public/stylesheets/ --source-map public/stylesheets/",
    "sass": "node-sass src/sass/main.scss -o public/stylesheets/ --source-map public/stylesheets/",
    "test": "npm run lint"
  },
  "dependencies": {
    "aws-param-store": "^3.0.0",
    "aws-sdk": "^2.463.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.16.1",
    "lodash": "^4.17.11",
    "node-sass": "^4.12.0",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.2",
    "react": "^16.8.0",
    "react-bootstrap": "^1.0.0-beta.9",
    "react-dom": "^16.8.0",
    "react-router-dom": "^5.0.1",
    "sequelize": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "eslint": "^6.0.1",
    "eslint-plugin-react": "^7.13.0",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.5.3",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8"
  }
}
